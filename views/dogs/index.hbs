<li><a href="/profiles/{{id}}">My Profile</a></li>
<h1>Browse Dogs</h1>
<form action="/dogs" method="get">
            <label for="search">Search</label>
            <input type="search" name="search" placeholder="Search" id="search">
        </form>
<ul class="dogList">
   {{#each dogs as |dog|}}
        <a href="/dogs/{{dog._id}}">
        <ul>
            <li>{{dog.name}}</li> 
            <li>{{dog.age}}</li>
            <li>{{dog.weight}}</li> 
            <li>{{dog.breed}}</li>
            <li><img src={{dog.avatar}}></li>
            <li>{{dog.additionalInfo}}</li>
        </ul>
        </a>
    {{/each}}
</ul>